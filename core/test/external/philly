            <script type="text/javascript">
//<![CDATA[

	// prototype function adds
	if(!String.prototype.trim){
		String.prototype.trim = function(){
			return this.replace(/^\s+|\s+$/g,'');
		};
	}
	if (!Array.prototype.indexOf){
		Array.prototype.indexOf = function(searchElement,fromIndex){
			var i,
				pivot = (fromIndex) ? fromIndex : 0, length;

			if (!this) {
				throw new TypeError();
			}

			length = this.length;

			if (length === 0 || pivot &gt;= length) {
				return -1;
			}

			if (pivot &lt; 0) {
				pivot = length - Math.abs(pivot);
			}

			for (i = pivot; i &lt; length; i++) {
				if (this[i] === searchElement) {
					return i;
				}
			}
			return -1;
		};
	}
	if (!Object.keys) {
		Object.keys = (function(){
			'use strict';
			var hasOwnProperty = Object.prototype.hasOwnProperty,
				hasDontEnumBug = !({toString: null}).propertyIsEnumerable('toString'),
				dontEnums = [
					'toString',
					'toLocaleString',
					'valueOf',
					'hasOwnProperty',
					'isPrototypeOf',
					'propertyIsEnumerable',
					'constructor'
				],
				dontEnumsLength = dontEnums.length;

			return function (obj) {
				if (typeof obj !== 'object' &amp;&amp; (typeof obj !== 'function' || obj === null)) {
					throw new TypeError('Object.keys called on non-object');
				}

				var result = [], prop, i;

				for (prop in obj) {
					if (hasOwnProperty.call(obj, prop)) {
						result.push(prop);
					}
				}

				if (hasDontEnumBug) {
					for (i = 0; i &lt; dontEnumsLength; i++) {
						if (hasOwnProperty.call(obj, dontEnums[i])) {
							result.push(dontEnums[i]);
						}
					}
				}
				return result;
			};
		}());
	}
	
	var selContent = '';
	var selQ = 'us';
	var timesArr = getLimitDates({'':'Forever', 1:'Past 24 Hours', 7:'Past 7 Days', 30:'Past 30 Days', 365:'Past Year'});
	var catsArr = {'':'All', 'news':'News', 'sports':'Sports', 'entertainment':'Entertainment', 'business':'Business', 'food':'Food', 'living':'Lifestyle', 'health':'Health'};
	var typesArr = {'':'All', 'article':'Articles', 'blog_post':'Blogs', 'video':'Videos', 'image_gallery':'Images'};
	var contentArr = {'inmeta:searchdate':timesArr, 'cg':catsArr, 'contenttype':typesArr};
	
	var selDate = '';
	if (selQ.match(/(inmeta:searchdate:[0-9-]*?\.\.(?:[0-9-]*?)?)(?:\+|%|$|&amp;)/))
		selDate = selQ.match(/(inmeta:searchdate:[0-9-]*?\.\.(?:[0-9-]*?)?)(?:\+|%|$|&amp;)/)[1];
	if (selDate!='')
		selQ = selQ.replace('\+' + selDate, '');
	
	var selLink = '/search?q='+selQ+'&amp;proxystylesheet=philly&amp;filter=0&amp;getfields=*';
	var dateLink = selLink + '&amp;requiredfields=' + selContent + '&amp;as_q=';
	if (selDate!='')
		selLink += '&amp;as_q=' + selDate;
	selLink += '&amp;requiredfields=';
	var modLink = '&amp;tlen=2048&amp;num=20';
	var timeForm = document.getElementById('filter-time-form');
	var $searchContainer = document.getElementById('search-filtered-by');
	var $clearFilterContainer = document.getElementById('search-filtered-clear');
	
	function printContentDate(elements, id, prefix) {
		var $container = document.getElementById('filter-'+id+'-list');
		var currDate = selDate;
		var selected = false;
		
		if (currDate.match(/[0-9.-]{12,14}/))
			currDate = currDate.replace(/[.]{2}/, '');
	
		for (i in elements) {
			var ele = elements[i];
			var eleLink = i;
			var currLink = dateLink;
			var conditional = (eleLink=='');
			var conditional2 = conditional;
			
			if (!conditional) {
				if (eleLink.charAt(0) == '0')
					eleLink = eleLink.substring(1);
			} else if (currLink.match(/(&amp;as_q=)($|&amp;)/))
				currLink = currLink.replace(/(&amp;as_q=)(?:$|&amp;)/, '');

			conditional = (eleLink == currDate);
			
			if (!conditional) {
				if (!conditional2)
					currLink += 'inmeta:searchdate:' + eleLink + '..';
				currLink += modLink;
				ele = '&lt;a href="'+ currLink +'"&gt;'+ ele +'&lt;/a&gt;';
			} else {
				ele = '&lt;span&gt;'+ ele +'&lt;/span&gt;';
				selected = true;
			}
			
			var $node = document.createElement('li');
			$node.innerHTML = ele;
			$container.appendChild($node);
		}
		
		if (selected === false)
			timeForm.className += ' selected';
	}
	function printContent(elements, id, prefix) {
		var $container = document.getElementById('filter-'+id+'-list');
		for (i in elements) {
			var ele = elements[i];
			var eleLink = i;
			var currLink = selLink;
			var currContent = selContent;
			var conditional = (!(currContent.match(new RegExp(prefix,"i"))));
			var conditional2 = conditional;
			if (eleLink!='') {
				eleLink = prefix + ":" + eleLink;
				conditional = currContent.match(new RegExp(eleLink, "gi"));
				if (currContent!='' &amp;&amp; !conditional)
					currContent += '.';
			}
			if (eleLink=='' &amp;&amp; !conditional2) {
				currContent = currContent.replace(new RegExp(prefix + ':([^.|&amp;]*)[|.]?', 'gi'), '').replace(/(?:[|.](?=\)|$|&amp;))|(\.\(\))/, '').replace(/^\(\)[.]/, '');
				if (eleLink!='' &amp;&amp; (currContent!='' &amp;&amp; !conditional))
					currContent += '.';
			}
			if (!conditional) {
				currLink += sortContent(currContent + eleLink);
				currLink += modLink;
				ele = '&lt;a href="'+ currLink +'"&gt;'+ ele +'&lt;/a&gt;';
			} else
				ele = '&lt;span&gt;'+ ele +'&lt;/span&gt;';
			
			var $node = document.createElement('li');
			$node.innerHTML = ele;
			$container.appendChild($node);
		}
	}
	function getLimitDates(dates){
		// gets initial length of array
		var datelen = Object.keys(dates).length - 1;
		
		// loops through array
		for (i in dates) {
			if (datelen &lt;= 0) break;
			if (i=='') continue;

			var oldI = i;
			var oldVal = dates[i];
			i = '0'+getLimitDate(i); //prepending 0 to prevent reordering of obj
			delete dates[oldI];
			dates[i] = oldVal;
			
			--datelen;
		}
		return dates;
	}
	function getLimitDate(n){
		var d = new Date();
		if (n === 1)
			d.setHours(d.getHours() - 24);
		else
			d.setDate(d.getDate() - n);
		return getFormatDate(d);
	}
	function getFormatDate(d){return d.getFullYear() + '-' + pad(d.getMonth()+1) + '-' + pad(d.getDate());}
	//function getFormatDate(d){return d.getFullYear() + pad(d.getMonth()+1) + pad(d.getDate()) + d.getHours();}
	function getNameFromDate(ele) {
		for (i in timesArr){				
			if (i.charAt(0) == '0' &amp;&amp; i.substring(1) == ele) // remove earlier prepended 0, where it exists
				ele = timesArr[i];
		}
		return ele;
	}
	function pad(n){n = n.toString();return (n.length &lt; 2) ? '0' + n : n;}
	function sortContent(content){
		if ( content == '(' )
			return '';
	
		var contents = content.split(/[().|]/).sort();
		var i = contents.length;
		var content = '';
		var prevfix = '';

		while (i--) {
			var x = contents[i];
			
			if (x == '')
				continue;
			
			var prefix = x.split(':');
			prefix = prefix[0];
			if (prevfix!=''){
				if (prevfix==prefix)
					content += '|';
				else
					content += ').(';
			} else if (prefix != '') {
				content += '(';
			}

			content += x;
			prevfix = prefix;
		}
		
		return (content == '(' ? '' : content + ')');
	}
	if (selDate!='') {
		selDate = selDate.replace('inmeta:searchdate:', '');
		var selArr = selDate.split(/[.]{2}/);
		var currLink = dateLink;
		var eleLink = '';
		var ele = '';
		
		if (currLink.match(/(&amp;as_q=)($|&amp;)/))
			currLink = currLink.replace(/(&amp;as_q=)(?:$|&amp;)/, '');

		if (selArr[1]) {
			for (i in selArr)
				selArr[i] = selArr[i].substring(5,7) + '/' + selArr[i].substring(8,10) + '/' + selArr[i].substring(0,4);
			ele = selArr.join(' - ');
		} else {
			ele = getNameFromDate(selArr[0]);
			if (ele.match(/[0-9]{8,12}/))
				ele = selArr[0].substring(4,6) + '/' + selArr[0].substring(6,8) + '/' + selArr[0].substring(0,4);
			else if (ele.match(/[0-9-]{10}/))
				ele = selArr[0].substring(5,7) + '/' + selArr[0].substring(8,10) + '/' + selArr[0].substring(0,4);
		}
		currLink += modLink;
		
		var $node = document.createElement('li');
		$node.innerHTML = '&lt;a href="'+ currLink +'"&gt;'+ele+'&lt;/a&gt;';
		$searchContainer.appendChild($node);
	}
	if (selContent!='') {
		var selArr = selContent.split(/[().|]/);
		var i = selArr.length;
		while (i--) {
			var eleLink = selArr[i];
			var delimiter = ':';
			var posA = 0;
			var posB = 1;
			
			var ele = eleLink.split(delimiter);
			var	tokens = ele.slice(posB);
			var eleName = tokens.join(delimiter);
				ele = ele.slice(posA,posB).join(delimiter);
			
			if (eleName) {			
				ele = contentArr[ele];
				ele = ele[eleName];
				
				var currContent = selContent.replace(new RegExp(eleLink+'[.|]?','i'), '').replace(/(?:[|.](?=\)|$|&amp;))|(\.\(\))/, '').replace(/^\(\)[.]/, '');
				var currLink = selLink + currContent + modLink;

				var $node = document.createElement('li');
				$node.innerHTML = '&lt;a href="'+ currLink +'"&gt;'+ele+'&lt;/a&gt;';
				$searchContainer.appendChild($node);
			}
		}
	}
	if (selContent!='' || selDate!='') {
		var q = 'us';
		var as_q = '';
		if (q.indexOf(as_q) != -1){
			q = q.replace(as_q, ''); // remove all as_q from q
			q = decodeURIComponent(q); //decodes uri
			q = q.replace(/\+/g, ' ');
			q = q.trim(); //trim spaces
			q = encodeURIComponent(q); //re-encodes uri
		} else if(q.indexOf(encodeURIComponent(as_q)) != -1) {
			encode_as_q = encodeURIComponent(as_q);
			q = q.replace(encode_as_q, ''); // remove all as_q from q
			q = decodeURIComponent(q); //decodes uri
			q = q.replace(/\+/g, ' ');
			q = q.trim(); //trim spaces
			q = encodeURIComponent(q); //re-encodes uri
		} else if(q.indexOf(decodeURIComponent(as_q)) != -1) {
			decode_as_q = decodeURIComponent(as_q);
			q = q.replace(decode_as_q, ''); // remove all as_q from q
			q = decodeURIComponent(q); //decodes uri
			q = q.replace(/\+/g, ' ');
			q = q.trim(); //trim spaces
			q = encodeURIComponent(q); //re-encodes uri
		}
		as_q = as_q.replace(new RegExp('[+]?(?:0)?inmeta:searchdate:([0-9.-]*)(?:[+]|%|&amp;|$)', 'i'), ''); // check for url-encoded spaces and remove any existing date data from as_q
		if (as_q!='')
			as_q = '&amp;as_q='+as_q;
		
		$clearFilterContainer.innerHTML = '&lt;a href="/search?q='+q+'&amp;amp;proxystylesheet=philly&amp;amp;filter=0&amp;amp;getfields=*'+as_q+modLink+'"&gt;Clear Filter&lt;/a&gt;';
	}
	
	printContentDate(timesArr, "time", "inmeta:searchdate");
	printContent(catsArr, "category", "cg");
	printContent(typesArr, "type", "contenttype");

//]]>
            </script>
          </div>
        </div>
