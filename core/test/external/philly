                    <strong>
                      ...
                    </strong>
                  </div>
                  <div class="meta">
                    <span>
                      <script type="text/javascript">
//<![CDATA[

				var mo_names = new Array("Blank","January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December");
				var dy_names = new Array("Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday");
	
				//var dd=document.getElementById("livedate").innerHTML;
				var dd='2014-03-16';
				var curr_year = dd.substr(0, dd.indexOf('-'));
				var curr_month = dd.substring(5,7);
				while(curr_month.charAt(0) === "0")
				curr_month = curr_month.substr(1);
				var curr_date = dd.substr(8, dd.indexOf('-'));
	
				document.write(mo_names[curr_month] + " " + curr_date + ", " + curr_year);
				
//]]>
                      </script>
                      March 16, 2014
                    </span>
                    <span>
                      Article
                    </span>
                    <span>
                      By Helen O'Neill
                    </span>
