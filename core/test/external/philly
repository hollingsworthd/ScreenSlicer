    <script type="text/javascript">
//<![CDATA[

		jQuery(document).ready(function($){
			var $timeForm = $('#filter-time-form'),
				$timeHeader = $timeForm.children('.subColHeader'),
				adblocks = ['lColSponsored', 'subColSponsored'],
				adlength = adblocks.length;
			function pad(n){n = n.toString();return (n.length &lt; 2) ? '0' + n : n;}
			function getRangeDates(d) {
				// mm/dd/yyyy = yyyy-mm-dd
				d = d.split('/');
				var d = d[2]+'-'+pad(d[0])+'-'+pad(d[1]);
				return d;
			}
			$('.datepick').datepicker().on('focus', function(){
				$(this).val('');
			});
			$timeHeader.on('click', function(){
				$timeForm.toggleClass('open');
			});
			$timeForm.submit(function(e){
				var $form = $(this),
					$q = $form.find('input[name="q"]'),
					$as_q = $form.find('input[name="as_q"]'),
					q = $q.val(),
					as_q = $as_q.val(),
					datePatt = new RegExp('[^/0-9]', 'gi'),
					dateFrom = $('#filter-time-from').val().replace(datePatt, ''),
					dateTo = $('#filter-time-to').val().replace(datePatt, '');
				
				if (q.indexOf(as_q) != -1){
					q = q.replace(as_q, ''); // remove all as_q from q
					q = q.replace(/\+/g, ' ');
					q = decodeURIComponent(q); //decodes uri
					q = $.trim(q); //trim spaces
				}
				as_q = as_q.replace(new RegExp('[+]?(?:0)?inmeta:searchdate:([0-9.-]*)(?:[+]|%|&amp;|$)', 'i'), ''); // check for url-encoded spaces and remove any existing date data from as_q
				
				if (dateTo=='(optional)')
					dateTo = '';
				datePatt = new RegExp('[0-9]{1,2}/[0-9]{1,2}/[0-9]{4}');
				if (!dateFrom.match(datePatt) || (dateTo &amp;&amp; !dateTo.match(datePatt))){
					alert('Please enter your dates in the suggested format.');
					return false;
				}
				dateFrom = getRangeDates(dateFrom);
				if (dateTo!='')
					dateTo = getRangeDates(dateTo);

				if (dateTo &amp;&amp; (dateFrom &gt; dateTo)) {
					var oldDateTo = dateTo;
					dateTo = dateFrom;
					dateFrom = oldDateTo;
				}
				
				var dateRange = 'inmeta:searchdate:'+ dateFrom +'..'+ dateTo; //daterange should always have .. on the end, whether or not dateTo exists
				
				if (as_q != '') // if as_q has data, prepend + spacer
					dateRange = '+' + dateRange;
					
				$q.val( q );
				$as_q.val( as_q + dateRange );
			});
		});
	
//]]>
    </script>
    <script type="text/javascript">
//<![CDATA[

		var googletag = googletag || {};
		googletag.cmd = googletag.cmd || [];
		(function(){
			var gads = document.createElement('script');
			gads.async = true;
			gads.type = 'text/javascript';
			var useSSL = 'https:' == document.location.protocol;
			gads.src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
			var node = document.getElementsByTagName('script')[0];
			node.parentNode.insertBefore(gads, node);
		})();
	
//]]>
    </script>
