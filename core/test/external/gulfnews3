    <script type="text/javascript">
//<![CDATA[

    	// Thumbnail Toolbar Buttons
        if (window.external.msIsSiteMode()) {
    		// Add buttons listener
        	if (document.addEventListener) {
           		 document.addEventListener('msthumbnailclick', onThumbnailButtonClicked, false);
            }
            else if (document.attachEvent) {
                document.attachEvent('onmsthumbnailclick', onThumbnailButtonClicked);
            };
    
            // Add buttons
            btnFb = window.external.msSiteModeAddThumbBarButton('/media/img/facebook.ico', 'Facebook');
            btnTw = window.external.msSiteModeAddThumbBarButton('/media/img/twitter.ico', 'Twitter');
    
            // Show thumbbar
            window.external.msSiteModeShowThumbBar();
    
            // Make the buttons enabled and visible.
            window.external.msSiteModeUpdateThumbBarButton(btnFb, true, true);
            window.external.msSiteModeUpdateThumbBarButton(btnTw, true, true);
        }
    
        function onThumbnailButtonClicked(e) {
            // Scroll to the next or previous page in the site
            var itemURL;
            switch (e.buttonID) {
                case btnFb:
                    itemURL = "http://www.facebook.com/pages/Gulf-News/143017562431177?sk=app_4949752878";
                    break;
                case btnTw:
                    itemURL = "http://twitter.com/#!/gulf_news";
                    break;       
            }
            window.open(itemURL);
        }
	
//]]>
    </script>
