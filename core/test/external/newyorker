         window._ghearst_vars = (function() {
        var
            metaKeyWords = (function(){
                var
                    i                  = 0,
                    metaKeywordContent = '',
                    nameAttribute      = null,
                    metaCollection     = document.getElementsByTagName('meta');

                for (i = 0; i < metaCollection.length; i++) {
                    nameAttribute = metaCollection[i].name.search(/keywords/);
                    if (nameAttribute!= -1) {
                        metaKeywordContent = metaCollection[i].content;
                        metaKeywordContent = metaKeywordContent.replace(/ /g,''); // to replace space
                    }
                }
                return encodeURIComponent(metaKeywordContent);
            })();

        if( (navigator.userAgent.match(/iPad/i) !== null) && (CN.cookie.get('ipadTakeover') !== 'true') ){
            return {
                'ams_ads_script_src': 'http://subscribe.newyorker.com/ams/page-ads.js?ad_category_prefix=search&browser_path=%2Fsearch&cat_prefixes=%2Csearch&keywords='+metaKeyWords+'&position_list=AMS_NYR_POPUP_FLOATINGAD%2CAMS_NYR_GLOBAL_NAVBAR_ROLLOVER%2CAMS_NYR_GLOBAL_RIGHTRAIL_A%2CAMS_NYR_GLOBAL_EMBEDDEDFORM%2CAMS_NYR_GLOBAL_RIGHTRAIL_C%2CAMS_NYR_GLOBAL_RIGHTRAIL_B%2CAMS_NYR_GLOBAL_HEADER%2CAMS_NYR_GLOBAL_NAVBAR%2CAMS_NYR_GLOBAL_HEADERCALLOUT%2CAMS_NYR_MOBILE_GLOBAL_FOOTER%2CAMS_NYR_GLOBAL_IPAD_TAKEOVER&site_prefix=newyorker&subdomain=www&url_name=search'
