				    var e = ezQuery(this);
				    var eh = ezQuery(this).html();
					var s2 = document.createElement("span");
					s2 = ezQuery(s2);
					s2.addClass("ez-ext");
					s2.append(ezQuery('<img src="http://www.foxnews.com/search-results/FileResource//themes/projects/foxnews/8.0_5580/images/external-source-bg.png">'));
				    e.html('<span>' + eh + '</span>');
					e.append(s2);
				});
				//add seps between meta data
				ezQuery(".ez-mainSearch .ez-itemMod-item .ez-meta p").each(function() {
				    var p = ezQuery(this);
				    if (!p.hasClass("ez-type") && !p.hasClass("ez-site")) {
				        p.before("<span>|</span>");
				    }
				});
				//move rss button above FB like button
			    ezQuery(".ez-fb-like-button").before(ezQuery(".ez-rss"));
				ezQuery(".ez-rss").css("display", "block");
				//add advertisement label to right rail ad
				ezQuery(".ez-ad-right").append('<p class="ez-ad-label">Advertisement</p>');
            });
        
