//<![CDATA[


CN.features.render({
    features : (function() {
        var
            i,
            feature = null,
            render = [],
            featuresOnPage = "siteheadertrendingstories    subnav    subnav    subnav    subnav    subnav    subnav    subnav leftalign whitebg alignleftborder mostviewed  whitebg  mostpopularfeatures  whitebg  alignleftgeorgia16 centeralign   divider3 thumbnailgrid".replace(/\s{2,}/g, ' ').split(' '),
            len = featuresOnPage.length,
            cssClass;

        featuresOnPage = jQuery.grep(featuresOnPage,function(el,idx){
            return jQuery.inArray(el, featuresOnPage) === idx;
        });
        for(i = 0; i &lt; len; i++){
            cssClass = featuresOnPage[i];
            if (CN.features.types.hasOwnProperty(cssClass)) {
                render.push(cssClass);
            }
        }
        return render;
    }())
});


