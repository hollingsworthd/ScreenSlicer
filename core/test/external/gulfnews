            $('#memberInfo').empty().append(
                '&lt;a href="http://checkout1.gulfnews.com/index.php/" title="Subscribe" style="color: white;background-color: red;padding: 3px 5px 3px 5px;font-weight: bold;"&gt;Subscribe&lt;/a&gt; | Welcome back, ' + json.loginStatus.userName + ' &lt;a href="/profile"&gt;My Profile&lt;/a&gt; | &lt;a href="/dologout?referrer=http://gulfnews.com/advanced-search/search-results"&gt;Sign Out&lt;/a&gt;');
        } else {
            $('#memberInfo').empty().append(
                '&lt;a href="http://checkout1.gulfnews.com/index.php/" title="Subscribe" style="color: white;background-color: red;padding: 3px 5px 3px 5px;font-weight: bold;"&gt;Subscribe&lt;/a&gt; | &lt;a href="/login" title="Register"&gt;Register&lt;/a&gt; | &lt;a href="/login?referrer=http://gulfnews.com/advanced-search/search-results" title="Sign In"&gt;Sign In&lt;/a&gt;');
        }
    }

