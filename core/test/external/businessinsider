if (typeof BI != 'undefined' &amp;&amp; typeof BI.showKontera != 'undefined' &amp;&amp; BI.showKontera === true) {
	// 1) Check for a "ke" cookie and set it = 1 with an 24 hour expire but ONLY if it is not present already
	if (!BI.cookies.get('ke')) {
		BI.cookies.set('ke',1,24); // (key, val, hrs)
	}

	// 2) insert kontera tag on the page but ONLY IF this is a *non-sponsored post* page.
	if (typeof post != 'undefined' &amp;&amp; typeof BI.post.sponsored === 'undefined') {
		// yeaaaaa globals
		window.dc_PublisherID = 107754;       // set to desktop ID
		window.dc_AdLinkColor = '#007705';    // set color for desktop

		if (navigator.userAgent.match(/iPhone|iPad|iPod|Android|Blackberry|IEMobile/i)) {
			window.dc_PublisherID = 225522 ;   // set to mobile ID
			window.dc_AdLinkColor = '#007705' ;   // set color for mobile
		}
	}
}

