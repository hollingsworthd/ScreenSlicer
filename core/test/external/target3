						        $.validator.addMethod("filterChars",function(value,element,params){
									
									// declare which special chars to validate
									var illegalChars = (params.illegalChars) ? params.illegalChars : "&lt;&gt;",
										enteredChars =[],
										splChars='';
									
									for (var i = 0; i &lt; value.length; i++)
									{
										charA = value.charAt(i);
										if (illegalChars.indexOf(charA) != -1 &amp;&amp; $.inArray(charA,enteredChars)==-1){
											enteredChars.push(charA);
										}	
									}
									if(enteredChars.length){ 
										
										splChars = enteredChars.join(' ').replace('&lt;','&amp;lt;').replace('&gt;','&amp;gt;');				
										$.validator.messages.filterChars  = "your password cannot include any of the following: " + splChars;
										return false; 
									}
									return true;
									
								});
								 
