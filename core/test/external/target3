    <script type="text/blzscript">
//<![CDATA[

var servicePlan = 0;
var serviceUrl;
var quantity = 0;
var amount = 0;

var changeQuantity = function () {
    quantity = parseInt($("#sevicePlanAddForm .qtynumber").val());
    amount = parseFloat($("#sevicePlanAddForm #servicePrice").text().replace(/\$/g,'')).toFixed(2);
};

var AddToCartNewLayout = function () {
      var multiple = $("#addtocart #ModalCarousel #multiItemList li.tile").length;
      if (multiple == 0) {
        multiple = 1;
      }
      $("#addtocart.overlay .addToCartHeavy.modalWindow .modalColumn.colType-main #ModalCarousel").after('&lt;div class="items-set"&gt;&lt;ul class="items"&gt;&lt;/ul&gt;&lt;/div&gt;');
      for (var i = 0; i &lt; multiple; i++) {
        $("#addtocart .items-set .items").append('&lt;li class="no-separator"&gt;&lt;div class="min"&gt;&lt;div class="image"&gt;&lt;a title="" href="#" class="mini-Image-Link"&gt; &lt;img class="mini-Image" src="" height="100px" width="100px" alt="" border="0"&gt;&lt;/a&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class="max"&gt;&lt;div class="details"&gt;&lt;p class="price"&gt;&lt;/p&gt;&lt;div class="oldPric"&gt;&lt;strong class="screen-reader-only"&gt;Original Price&lt;/strong&gt;reg: &lt;span class="strike"&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class="link"&gt;&lt;/p&gt;&lt;p class="quantity"&gt;&lt;/p&gt;&lt;/div&gt;	&lt;/div&gt;&lt;/li&gt;');
        $("#addtocart .items-set .mini-Image").eq(i).attr("src", $("#addtocart .cartProductImage img").eq(i).attr("src"));
        $("#addtocart .items-set .mini-Image-Link").eq(i).attr("href", $("#addtocart .cartProductImage a").eq(i).attr("href"));
        $("#addtocart .tileInfo .sale").eq(i).children().remove();
        $("#addtocart .items-set .max .details .price").eq(i).text($("#addtocart .tileInfo .sale").eq(i).text());
        $("#addtocart .items-set .max .details .oldPric .strike").eq(i).text($("#addtocart .tileInfo .oldPric .strike").eq(i).text());
        $("#addtocart .tileInfo .productTitle").eq(i).clone().appendTo("#addtocart .items-set .max .details .link:eq("+i+")");
        $("#addtocart .items-set .max .details .quantity").eq(i).text($("#addtocart .tileInfo .productInfoUnit .value.qty").eq(i).text());
        $("#addtocart .items-set .max .details .quantity").eq(i).prepend('&lt;span&gt;quantity: &lt;/span&gt;');
        $("#addtocart .tileInfo .extra").eq(i).clone().insertBefore("#addtocart .items-set .max .details .quantity:eq("+i+")");
        if ($("#addtocart .tileInfo .oldPric .strike").eq(i).length == 0) {
            $("#addtocart .items-set .max .details .oldPric").eq(i).remove();
        }
        if (i+1 == multiple) {
            $("#addtocart .items-set .items li").eq(i).addClass("last");
        }
        if ($("#addtocart .items-set .max .details .link:eq("+i+")").text().indexOf('Service Plan') &gt; -1 || $("#addtocart .items-set .max .details .link:eq("+i+")").text().indexOf('Replacement Plan') &gt; -1) {
            $("#addtocart .items-set .items li").eq(i).addClass('blue');
            $("#addtocart .items-set li .min .image img").eq(i).css({"height":"75px","width":"75px"});
        }
        if ($("#addtocart .items-set .items .max .extra").eq(i).text().indexOf('gift card') &gt; -1) {
            giftcardamount = $("#addtocart .items-set .items .max .extra").eq(i).text().match(/\d+/g)[0];
            $("#addtocart .items-set .items").append('&lt;li class="no-separator last blue"&gt;&lt;div class="min"&gt;&lt;div class="image"&gt;&lt;a title="" href="/p/iconic-puppy-gift-card/-/A-11096774#prodSlot=medium_1_1" class="mini-Image-Link"&gt;&lt;img class="mini-Image" src="http://img3.targetimg3.com/wcsstore/TargetSAS//img/p/11/09/11096774_138x138.jpg" height="75px" width="75px" alt="" border="0"&gt;&lt;/a&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class="max"&gt;&lt;div class="details"&gt;&lt;p class="price"&gt;FREE&lt;/p&gt;&lt;div class="oldPric"&gt;with purchase&lt;/div&gt;&lt;p class="link"&gt;&lt;a href="http://www.target.com/p/iconic-puppy-gift-card/-/A-11096774" title="Iconic Puppy Gift Card" id="prodTitle-medium-1-1" class="productClick"&gt;Free $'+giftcardamount+' gift card with purchase&lt;/a&gt;&lt;/p&gt;&lt;p class="quantity"&gt;&lt;span&gt;quantity: &lt;/span&gt;1&lt;/p&gt;&lt;/div&gt;&lt;/div&gt;&lt;/li&gt;');
            if ($("#addtocart .items-set .items li").eq(i).hasClass("last")) {
               $("#addtocart .items-set .items .last").removeClass('last');
               $("#addtocart .items-set .items li").eq(i+1).addClass('last');
            }
        }
      }
      if ($("#sevicePlanAdd").hasClass('state-disabled')) {
        $("#sevicePlanAddForm").hide();
      }
      $("#addtocart.overlay.modal").show();
};

var ChangeText = function () {
    $("#crossSell-atc").remove();
    $("#addtocart .cartAddAnnounce").empty().append('&lt;h2 class="modalHead  aria-title"&gt;added to cart&lt;/h2&gt;');
    $("#modalClose").insertBefore("#checkOutLink");
    $("#addtocart .addToCartHeavy .col-right").appendTo("#addtocart .addToCartHeavy");
    $("#addtocart .addToCartHeavy .col-right").attr("class","TestColRight");
    $("#addtocart .addToCartHeavy .TestColRight .legalnote").remove();
    $("#addtocart .TestColRight h3:last").text($("#addtocart .TestColRight h3:last").text().match(/\d+/g)[0] + ' items in cart');
    $("#addtocart .TestColRight h3").after('&lt;h2&gt;'+$("#addtocart .TestColRight .subtotalInfo .subtotal").text()+'&lt;/h2&gt;');
    $("#addtocart .TestColRight .subtotalInfo").remove();
    $("#addtocart #sevicePlanAddForm .addtocartsrvpln .cartCol3").wrapInner('&lt;div class="essentials"&gt;&lt;/div&gt;');
    $("#addtocart #sevicePlanAddForm .addtocartsrvpln .cartCol3").prepend('&lt;div class="image"&gt;&lt;a title="" href="/p/target-3-year-service-plan-covers-items-500-00-999-99/-/A-11691841?lnk=atc_overlay" class="mini-Image-Link"&gt; &lt;img class="mini-Image" src="http://Img1.targetimg1.com/wcsstore/TargetSAS//img/p/11/69/11691841_130316083000_106x106.jpg" height="75px" width="75px" alt="" border="0"&gt;&lt;/a&gt;&lt;/div&gt;');
    changeQuantity();
    amount = parseFloat($("#sevicePlanAddForm #servicePrice").text().replace(/\$/g,'')).toFixed(2);
    serviceUrl = $("#addtocart #sevicePlanAddForm .srvseedetials");
    if ($("#addtocart #sevicePlanAddForm .srvseedetials").text().indexOf('Replacement Plan') &gt; -1) {
        serviceImage = "http://img2.targetimg2.com/wcsstore/TargetSAS/img/p/11/69/11691836_130315111335_138x138.jpg";
    }
    else if ($("#addtocart #sevicePlanAddForm .srvseedetials").text().indexOf('Gaming') &gt; -1) {
        serviceImage = "http://img1.targetimg1.com/wcsstore/TargetSAS/img/p/14/35/14350376_121127105636_138x138.jpg";
    }
    else if ($("#addtocart #sevicePlanAddForm .srvseedetials").text().indexOf('Service Plan') &gt; -1) {
        serviceImage = "http://img1.targetimg1.com/wcsstore/TargetSAS/img/p/11/69/11691839_130315111335_138x138.jpg";
    }
    else {
        serviceImage = "http://img1.targetimg1.com/wcsstore/TargetSAS/img/p/14/34/14349950_121127105636_138x138.jpg";
    }
    $("#sevicePlanAdd").click(function () {
        servicePlan = 1;
        $("#addtocart .TestColRight h3:last").text((parseInt($("#addtocart .TestColRight h3:last").text().match(/\d+/g)[0])+quantity) + ' items in cart');
        $("#addtocart .TestColRight h2").text("subtotal: $" + (parseFloat($("#addtocart .TestColRight h2").text().match(/[0-9,]+\.[0-9]+/g)[0].replace(/,/g,''))+parseFloat(amount)).toFixed(2).replace(/(\d+)(\d{3})/, '$1'+','+'$2'));
    });
    $("#sevicePlanAddForm .selectboxwrap .downArrow").click(function () {
        setTimeout(changeQuantity, 10);
    });
    $("#sevicePlanAddForm .selectboxwrap .upArrow").click(function () {
        setTimeout(changeQuantity, 10);
    });
    AddToCartNewLayout();
};

$(document).ajaxComplete(function (event, xhr, settings) {
    if (settings.url.indexOf('checkout_itemadd') &gt; -1 &amp;&amp; !servicePlan) {
        setTimeout(ChangeText, 400);
    }
    else if (servicePlan == 1) {
        servicePlan = 0;
        $("#addtocart .items-set .items .last").removeClass('last');
        $("#addtocart .items-set .items").append('&lt;li class="no-separator last blue"&gt;&lt;div class="min"&gt;&lt;div class="image"&gt;&lt;a title="" href="/p/target-3-year-service-plan-covers-items-500-00-999-99/-/A-11691841?lnk=atc_overlay" class="mini-Image-Link"&gt; &lt;img class="mini-Image" src="'+serviceImage+'" height="75px" width="75px" alt="" border="0"&gt;&lt;/a&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class="max"&gt;&lt;div class="details"&gt;&lt;p class="price"&gt;$'+amount+'&lt;/p&gt;&lt;p class="link"&gt;'+serviceUrl.html()+'&lt;/p&gt;&lt;p class="quantity"&gt;&lt;span&gt;quantity: &lt;/span&gt;'+quantity+'&lt;/p&gt;&lt;/div&gt;&lt;/div&gt;&lt;/li&gt;');
    }
});

//]]>
    </script>
