              <script type="text/javascript">
//<![CDATA[

 function checkFields(field, event) {
        var keyCode = event.keyCode ? event.keyCode : event.which ? event.which : event.charCode;
        if (keyCode == 13) {
            if (document.getElementById('street').value == "" || document.getElementById('street').value == "Street Address") {
                alert('Please enter a valid street address.');
                return false;
            } else if (document.getElementById('zipCode').value == "" || document.getElementById('zipCode').value == "Zip Code") {
                alert('Please enter a valid zip code.');
                return false;
            } else {
                document.forms.addressForm.submit();
            }
        }
    }
    function checkFieldsButton() {
        if (document.getElementById('street').value == "" || document.getElementById('street').value == "Street Address") {
            alert('Please enter a valid street address.');
            return false;
        } else if (document.getElementById('zipCode').value == "" || document.getElementById('zipCode').value == "Zip Code") {
            alert('Please enter a valid zip code.');
            return false;
        } else {
            document.forms.addressForm.submit();
        }
    }

//]]>
