      <script type="text/javascript">
//<![CDATA[

    
    // --- Session timeout notification and expiration script ---

    var jsTimeoutNotify;
    var jsTimeoutExpire;

    var sessTimeoutExpire;  //time until expiration
    var sessTimeoutNotify;  //time until warning; 

            
    //Initial load and partial postbacks initiate/refresh the session
    function pageLoad() {            
        
        sessTimeoutExpire = 0;  
        sessTimeoutNotify = sessTimeoutExpire - 300000; // (5 minutes = 300,000 milliseconds)   

        if (sessTimeoutExpire &gt; 0)
        {             
            if(jsTimeoutExpire)
            {
                clearTimeout(jsTimeoutExpire);   
                clearTimeout(jsTimeoutNotify);   
            }//if
                
            jsTimeoutNotify = setTimeout("TimeoutNotifaction()", sessTimeoutNotify );
            jsTimeoutExpire = setTimeout("TimeoutExpiration()", sessTimeoutExpire );     
        }   // if   
    } //pageLoad()         
    
    function TimeoutNotifaction()
    {
        $.openModal({ target: '#avlModal1', width: '400px', height: '130px' });                 
    }   // TimeoutNotifaction()   

    function LogIn()
    {        
         window.location = "/Login.aspx";
    }   //LogIn()
       
    function TimeoutExpiration()
    {
        $.closeModal({ target: '#avlModal1'});          
        var x = setTimeout("$.openModal({ target: '#avlModal2', width: '340px', height: '120px' })", 510 );         
    }   // TimeoutExpiration()
  

//]]>
      </script>
    </form>
