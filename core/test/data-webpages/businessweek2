  BBBW_comScore.params = {};
  if (_BUCKET != "" &amp;&amp; _BUCKET_GROUP != ""){
    BBBW_comScore.params['bb_bucket']       = _BUCKET;
    BBBW_comScore.params['bb_bucket_group'] = _BUCKET_GROUP;
  }
  BBBW_comScore.track();
  BBBW_comScore.write_cookie(comScore_track_key,"",-1);

//]]>
    </script>
