function rp_resize(id, sz)
{
var el, w, h;
w = sz.substring(0, sz.indexOf('x'));
h = sz.substring(sz.indexOf('x') + 1, sz.length);
d = document.getElementById(id);
if (!d.width) {
d.setAttribute('width', parseInt(w));
} else {
d.width = parseInt(w);
}
if (d.style.width) {
d.style.width = parseInt(w)+'px';
}
if (!d.height) {
d.setAttribute('height', parseInt(h));
} else {
d.height = parseInt(h);
}
if (d.style.height) {
d.style.height = parseInt(h)+'px';
}
if (document.getElementById('mpu') &amp;&amp; document.getElementById('mpu').className === 'skyscraper') {
document.getElementById('mpu').style.height = parseInt(h)+'px';
}
