                <script type="text/javascript">
//<![CDATA[

//&lt;![CDATA[
function PlaceHolder(){
var normalItems = new Array(
["reg-email", "EMAIL"],
["log-email", "EMAIL"]
);
var passwordItems = new Array(
["dummy-reg-password", "reg-password"],
["dummy-reg-password2", "reg-password2"],
["dummy-log-password", "log-password"]
);
function selectPasswordItems(){
$(passwordItems).each(function(i){
passwordType(passwordItems[i][0], passwordItems[i][1]);
});
}
function selectNormalItems(){
$(normalItems).each(function(i){
normalType(normalItems[i][0], normalItems[i][1]);
});
}
function normalType(c, d){
var elementId = $("#"+c);
var elementValue = d;
elementId.addClass("newInputStyle");
elementId.blur(function(){
var value=$(this).val();
//$(this).addClass("placeHolderActive");
/* if(value!=elementValue){
$(this).removeClass("placeHolderActive");
}*/
if(value==elementValue){
$(this).addClass("placeHolderActive");
}
if(value==""){
$(this).addClass("placeHolderActive");
}
elementId.focus(function(){
$(this).removeClass("placeHolderActive");
var value=$(this).val();
if(value==elementValue){
$(this).val("");
$(this).blur(function(){
if($(this).val()==""){
$(this).val(elementValue);
}
});
}
});
});
}
function passwordType(a, b){
var dummyElement = $("#"+a);
var realElement = $("#"+b);
dummyElement.addClass("newInputStyle");
realElement.addClass("newInputStyle");
realElement.hide();
dummyElement.focus(function(){
if(dummyElement.val() === "PASSWORD" || dummyElement.val() === "CONFIRM PASSWORD"){
realElement.css("display", "inline");
dummyElement.hide();
realElement.focus();
}
});
realElement.blur(function(){
if($(this).val() ==""){
$(this).hide();
dummyElement.css("display", "inline");
}
});
}
function closeLightbox(){
$('.ui-dialog-titlebar-close').click(function(){
$('#reg-email').val("EMAIL");
$('#log-email').val("EMAIL");
});
$('#gigya-join-now-link').click(function(){
$('#log-email').val("EMAIL");
})
}
selectNormalItems();
selectPasswordItems();
closeLightbox();
}
$(function () {
EVS.gigyaLinksToggle('#GigyaShowLoggedIn', '#GigyaShowLoggedOut');
var gigyaPlaceHolder = new PlaceHolder();
});
//]]&gt;

//]]>
                </script>
