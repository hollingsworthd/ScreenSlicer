s.prop30 = s.prop31 = s.events = '';
});
// Custom link tracking for IV promo widget, bind only if we find node in the DOM.
$('.ivPromo-miniCarousel .voicescarousel').bind('click', function(e) {
var s=s_gi(s_account), type = $('meta[property=og:type]').attr('content');
if( typeof type === 'undefined' ) {
