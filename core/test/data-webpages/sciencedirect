    <script type="text/javascript">
//<![CDATA[


 function pageNumberValidation(e, form, hiddenField){
   var isNS = (navigator.appName=="Netscape")?1:0;
   var key;

   if (!isNS) {
     key = e.keyCode;
   } else {
     key = e.which;
   }

   if(key == 8 || key == 0){
     return true;
   } else if (key > 47 && key < 58) {
      return true; 
   }else if(key == 13) {
      hiddenField.value = "true";
      form.submit();
   }
   return false;
}


//]]>
    </script>
