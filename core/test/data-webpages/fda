//<![CDATA[

      function keyPressEvent(e)
      {
      
      	 if (e.keyCode == 13)
      {
      subSearchForm();
      
      }
      		
      		
      }
      function subSearchForm()
      {
      	/* Set the variable for the remainder of Q based on the input field. If the variable is empty set the URL string to only
      use Q, otherwise include the filetype */
      	var filtypeforQ = document.getElementById('restOfQ2').value;
      
      	 if (filtypeforQ != '')
      	{
      		concatQ = document.gs.q.value + '%20filetype' + document.getElementById('restOfQ2').value;
      
      	}
      	else
      	{
      		concatQ = document.gs.q.value;
      
      	}
      
      	if (document.gs.partialfields.value != '')
      	{
      		partFieldsVar = '&partialfields=' + document.gs.partialfields.value;
      	}
      	
      	else
      	{
      		partFieldsVar = '';
      	}
      
      	window.location = '/search?q=' +  concatQ + '&client=' + document.gs.client.value + '&proxystylesheet=' + document.gs.proxystylesheet.value
      + '&output=' + document.gs.output.value + '&site=' + document.gs.site.value +'&requiredfields=' + document.gs.requiredfields.value
      + partFieldsVar + '&sort=' + document.gs.sort.value + '&filter=' + document.gs.filter.value;
      
      
      
      }
      
      function SetRestrict()
      {	
      	finalStr="";
      	
      	for(i=0; i<15; i++)
      	{
      			if (document.f.restrictBox[i].checked)
      			{
      				finalStr = finalStr + document.f.restrictBox[i].value + "|";
      			}
      	}
      	if (finalStr == "") 
      		finalStr = "FDAgov";
      	else
      		document.f.filter.value = "0";
      	
      	document.f.site.value = finalStr.replace(/\|$/, '');
      }
      
//]]>
    </script>
