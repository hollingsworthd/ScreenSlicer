	   $('#fda_ops_btn').addClass('select');
	   $('.filterSelected').css('display','none');
	   $('#fda_opsSelected').css('display','inline');
    }	
    if(window.location.href.indexOf("search_topic:FDA%20Operations") > -1) {
	   $('#clear_btn').removeClass('select');
 	   $('#fda_ops_btn').addClass('select');
	   $('.filterSelected').css('display','none');
	   $('#fda_opsSelected').css('display','inline');
    }
    if(window.location.href.indexOf("search_topic:FDA Operations") > -1) {
	   $('#clear_btn').removeClass('select');
 	   $('#fda_ops_btn').addClass('select');
	   $('.filterSelected').css('display','none');
	   $('#fda_opsSelected').css('display','inline');
    }	
    if(window.location.href.indexOf("search_topic:FDA+Operations") > -1) {
	   $('#clear_btn').removeClass('select');
 	   $('#fda_ops_btn').addClass('select');
	   $('.filterSelected').css('display','none');
	   $('#fda_opsSelected').css('display','inline');
    }	
    if(window.location.href.indexOf("site=FDAgov-Category-WarningLetters") > -1) {
	   $('#clear_btn').removeClass('select');
 	   $('#fda_warningletters_btn').addClass('select');
	   $('.filterSelected').css('display','none');
	   $('#fda_warninglettersSelected').css('display','inline');
    }
    if(window.location.href.indexOf("site=FDAgov-Section-AdvisoryCommittees") > -1) {
	   $('#clear_btn').removeClass('select');
 	   $('#fda_advisorycommittees_btn').addClass('select');
	   $('.filterSelected').css('display','none');
	   $('#fda_advisorycommitteesSelected').css('display','inline');
   }

/* Filter by Format */
    if(window.location.href.indexOf("filetype%3Ahtm") > -1) {
	   $('#clearFormat_btn').removeClass('selectFormat');
	   $('#web_btn').addClass('selectFormat');
	   $('.formatSelected').css('display','none');
	   $('#webSelected').css('display','inline');
	}
    if(window.location.href.indexOf("filetype:htm") > -1) {
	   $('#clearFormat_btn').removeClass('selectFormat');
	   $('#web_btn').addClass('selectFormat');
	   $('.formatSelected').css('display','none');
	   $('#webSelected').css('display','inline');
	}	
    if(window.location.href.indexOf("filetype%3Apdf") > -1) {
	   $('#clearFormat_btn').removeClass('selectFormat');
	   $('#pdf_btn').addClass('selectFormat');
	   $('.formatSelected').css('display','none');
	   $('#pdfSelected').css('display','inline');
	}
    if(window.location.href.indexOf("filetype:pdf") > -1) {
	   $('#clearFormat_btn').removeClass('selectFormat');
	   $('#pdf_btn').addClass('selectFormat');
	   $('.formatSelected').css('display','none');
	   $('#pdfSelected').css('display','inline');
	}	
    if(window.location.href.indexOf("filetype%3Adoc") > -1) {
	   $('#clearFormat_btn').removeClass('selectFormat');
	   $('#doc_btn').addClass('selectFormat');
	   $('.formatSelected').css('display','none');
	   $('#docSelected').css('display','inline');
	}
    if(window.location.href.indexOf("filetype:doc") > -1) {
	   $('#clearFormat_btn').removeClass('selectFormat');
	   $('#doc_btn').addClass('selectFormat');
	   $('.formatSelected').css('display','none');
	   $('#docSelected').css('display','inline');
	}	
    if(window.location.href.indexOf("filetype%3Axls") > -1) {
	   $('#clearFormat_btn').removeClass('selectFormat');
	   $('#xls_btn').addClass('selectFormat');
	   $('.formatSelected').css('display','none');
	   $('#xlsSelected').css('display','inline');
	}
    if(window.location.href.indexOf("filetype:xls") > -1) {
	   $('#clearFormat_btn').removeClass('selectFormat');
	   $('#xls_btn').addClass('selectFormat');
	   $('.formatSelected').css('display','none');
	   $('#xlsSelected').css('display','inline');
	}	
   if(window.location.href.indexOf("filetype%3Appt") > -1) {
	   $('#clearFormat_btn').removeClass('selectFormat');
	   $('#ppt_btn').addClass('selectFormat');
	   $('.formatSelected').css('display','none');
	   $('#pptSelected').css('display','inline');
	}
   if(window.location.href.indexOf("filetype:ppt") > -1) {
	   $('#clearFormat_btn').removeClass('selectFormat');
	   $('#ppt_btn').addClass('selectFormat');
	   $('.formatSelected').css('display','none');
	   $('#pptSelected').css('display','inline');
	}	
	
/* Sort by */
    if(window.location.href.indexOf("sort=date%3AD%3AL%3Ad1") > -1) {
	   $('#rdate_btn').removeClass('selectSort');
	   $('#relevance_btn').addClass('selectSort');
	   $('.sortSelected').css('display','none');
	   $('#relevanceSelected').css('display','inline');
	}
    if(window.location.href.indexOf("sort=date:D:L:Ad1") > -1) {
	   $('#rdate_btn').removeClass('selectSort');
	   $('#relevance_btn').addClass('selectSort');
	   $('.sortSelected').css('display','none');
	   $('#relevanceSelected').css('display','inline');
	}	
	if(window.location.href.indexOf("sort=date%3AD%3AS%3Ad1") > -1) {
	   $('#relevance_btn').removeClass('selectSort');
	   $('#rdate_btn').addClass('selectSort');
	   $('.sortSelected').css('display','none');
	   $('#rdateSelected').css('display','inline');
	}
	if(window.location.href.indexOf("sort=date:D:S:d1") > -1) {
	   $('#relevance_btn').removeClass('selectSort');
	   $('#rdate_btn').addClass('selectSort');
	   $('.sortSelected').css('display','none');
	   $('#rdateSelected').css('display','inline');
	}	


});


//address the filetype being removed from the Q value in URL
function stopRKey(evt) {
  var evt = (evt) ? evt : ((event) ? event : null);
  var node = (evt.target) ? evt.target : ((evt.srcElement) ? evt.srcElement : null);
  if ((evt.keyCode == 13) && (node.id=="q_field") ) {return false;}
}

document.onkeypress = stopRKey; 

