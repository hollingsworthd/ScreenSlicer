//<![CDATA[


function updateSearchField(){
	var frmCount = document.forms.length;
	for(i=0; i<frmCount; i++){
		var frm = document.forms[i];
		var frmElemCount = frm.elements.length;
		for(j=0; j<frmElemCount; j++){
			if(frm.elements[j].type=="text" && (frm.elements[j].name=="Ntt")){
				//frm.action = "http://search.time.com/results.html";
				if(searchString!=null && searchString != ""){// && frm.elements[j].value==""){		
					
					frm.elements[j].value=searchString;
				}
			}
		}
	}
}

String.prototype.trim = function () {
    return this.replace(/^\s*/, "").replace(/\s*$/, "");
}

var searchBoxVal = "Search Time.com";
function searchBoxValue(txtField){
	if(txtField.value == searchBoxVal || txtField.value.trim()==""){
		txtField.value = "";
		return false;
	}else{
		return true;
	}
}

function processSTerm(parentFrm){
	if(parentFrm != null && parentFrm.Ntt.value.trim() != ""){
		encodeURIComponent(paremtFrm.Ntt.value.trim());
	}
}
