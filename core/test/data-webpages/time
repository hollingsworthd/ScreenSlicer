            <script type="text/javascript" defer="defer">
//<![CDATA[

			function RangeFilter_tmp() {
				var url = document.date_filter_form.curr_url.value;
				var from_day 	= "01";
				var from_month 	= document.date_filter_form.from_month.value;
				var from_year 	= document.date_filter_form.from_year.value;
				var to_day 	= "31";
				var to_month 	= document.date_filter_form.to_month.value;
				var to_year 	= document.date_filter_form.to_year.value;
				to_day = daysInMonth(to_month, to_year);
				
				var newurl = url+"&Nf=p_date_range%7cBTWN+"+from_year+from_month+from_day+"+"+to_year+to_month+to_day;
				location.search = newurl;
			}
			function daysInMonth(month,year) {
				var dd = new Date(year, month, 0);
				return dd.getDate();
			} 
		
//]]>
            </script>
