                function enableFeedbackLinkDiv() {
                   amznJQ.onReady('jQuery', function() {
                     jQuery('#ShowFeedbackLinkDiv').show();
                   });
                }
                enableFeedbackLinkDiv();
                var flag = 1;
                function showSLF() {
                     if( flag == 1) {
                       // Make a request to the handler with new ads 
                       jQuery.post('/gp/product/handler.html', { 'sid': '185-1721835-4911162','jsonAds' : escape(jsonAds) } ,function(data) {
                           jQuery('#FeedbackFormDiv').html(data);
                           jQuery('#FeedbackFormDiv').show();
                           jQuery('#ShowFeedbackLinkDiv').hide();
                           jQuery('#ResponseFeedbackLinkDiv').hide(); 
                           flag = 0;
                       });
                     } else {
                       jQuery('#reports-ads-abuse-form').show();
                       jQuery('#FeedbackFormDiv').show();
                       jQuery('#reports-ads-abuse').show();
                       jQuery('#ShowFeedbackLinkDiv').hide();
                       jQuery('#ResponseFeedbackLinkDiv').hide(); 
                     }
                }

