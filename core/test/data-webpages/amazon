browseNodeMap.n229534 = "softwareBN";
       
        window.pa_asins_tower;
        window.pa_sa_tower;
        window.pa_bl_tower;
        window.pa_kw_tower;
       
        var lazyLoadURL = '/gp/product-ads/lazyLoad/handler/click-within-handler.html';
        var cw = {};
          amznJQ.available('search-js-general', function() {
            amznJQ.available('search-js-autocomplete', function() {
              SPUtils.afterEvent('searchajax', function() {    
                var bl;
                if (jQuery.pageContext && jQuery.pageContext.browseLadder) {
                  bl = unescape(jQuery.pageContext.browseLadder);
                }   
                var isBrowse = 0;

                  var cwurl = location.href;
                  var regexRh =  /([?&=])(rh=)([^&]*)/;
                  var regexField = /([?&=])(field-brandtextbin=)([^&]*)/;                  
                  var regexSort = /([?&=])(sort=)([-\w]+)/; 
                  var regexMe = /([?&=])(me=)([^&]*)/; 
                  var regexScn = /([?&=])(scn=)([-\w]+)/; 
                  var hashindex = cwurl.indexOf("#"); 
                  var rhstring;
                  var sortstring;
                  if (hashindex == -1) 
                  {
                    rhstring = regexRh.exec(cwurl);
                    sortstring = regexSort.exec(cwurl);
                  } else {   
                    hashstring = cwurl.substring(hashindex+1);
                    rhstring = regexRh.exec(hashstring);
                    sortstring = regexSort.exec(hashstring);
                  }
                 
                  function cw_extractParams(inputString) {
                    var paramString = unescape(inputString); 
                    var eParams = paramString.split(",");
                    return eParams; 
                  } 
                  if (rhstring != null && rhstring.length != 0) 
                  {
                    var rhParams = cw_extractParams(rhstring);       
                    var pat = /(\b[n|k|i]\b)/; 
                    for (var x = 0; x < rhParams.length; x++) {
                      var entry, rhKey;
                      entry = rhParams[x].split(":"); 
                      if (entry.length == 2) {
                        rhKey = entry[0];                 
                        if(!pat.test(rhKey)) { 
                          jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                          return;
                        }
                      } 
                    }
                  }
                  if (regexField.test(unescape(cwurl))) { 
                    jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                    return;
                  }
                  if(regexMe.test(unescape(cwurl))) { 
                    jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                    return;
                  }
                
                  if(sortstring != null && sortstring.length != 0) {
                    var sortParams = cw_extractParams(sortstring);         
                    for (var x = 0; x < sortParams.length; x++) { 
                      var entry, sortVal;
                      entry = sortParams[x].split("=");
                      if (entry.length == 2) {
                        sortVal = entry[1];
                        if(sortVal != "relevancerank" && sortVal != "") {
                          jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                          return;
                        }
                      }  
                    }
                  }

                  if (typeof atfAdAsinList !== 'undefined') {
                     window.pa_asins_tower = atfAdAsinList;
                  } else if((typeof(window.pa_bl_tower) != "undefined" && window.pa_bl_tower != bl) || (typeof(window.pa_kw_tower) != "undefined" && window.pa_kw_tower != iss.keyword())) {
                      window.pa_asins_tower = undefined;
                  } else if(typeof(asinList) != "undefined") {
                    if(asinList == 0) {
                      jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                      return;
                    } else if (typeof(window.pa_asins_tower) != "undefined") { 
                      window.pa_asins_tower = window.pa_asins_tower + asinList;                       
                    } else {
                      window.pa_asins_tower = asinList; 
                    }
                    
                    var temp = window.pa_asins_tower.split(",");
                    if(temp.length > 40 ) {
                      var extra = temp.length - 40; 
                      for (var i = 0; i < extra; i++) {
                        temp.shift();
                      }
                    } 
                    window.pa_asins_tower = temp.join(); 
                  }  
  
                window.pa_bl_tower = bl; 
                window.pa_kw_tower = iss.keyword();
                window.pa_sa_tower = SearchPageAccess.searchAlias();
                         
              if (bl == null || bl.length == 0) {
                jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                return; 
              }
              
              var suggestedBN = [];
              if (bl == 'i:aps') {
                jQuery("#refinements > ul > li > a:lt(3)").each(function(){
                 var urlattr=jQuery(this).attr("href");
                var suggestedBnUrl = regexRh.exec(urlattr);
                var suggestedBnParams = cw_extractParams(suggestedBnUrl);
                for (var i=0; i < suggestedBnParams.length; i++) {
                  var sEntry;
                  sEntry = suggestedBnParams[i].split(":");
                  if (sEntry.length == 2 && sEntry[0] == 'n') {
                        suggestedBN.push(sEntry[1]);
                  } 
                }
                });
              }

              var bnlist = bl.split(',');
              var bn = bnlist[0].replace(":","");          
              var kw = iss.keyword();
              var sa = SearchPageAccess.searchAlias();
              if (!kw) {
                kw = jQuery("#refinements > ul > li > strong").html();
                isBrowse = 1;
              }

              if (
                  !(browseNodeMap.hasOwnProperty(bn) || bl == 'i:aps') ||
                  /* We are not under the allowed list of browse-nodes */
                  !kw)                        
              {
                jQuery('#click_withinLazyLoad_tower').html("<div></div>"); 
                return;
              }

              cw.sa = sa;
              cw.kw = kw;
              cw.bl = bl;

              jQuery.ajax({
                
                url: lazyLoadURL + "?searchTerms="+encodeURIComponent(cw.kw)+"&searchAlias="+encodeURIComponent(cw.sa)+
                                   "&isDebug=0&widgetName="+encodeURIComponent(widgetName)+
                                   "&pageType=Search"+"&subPageType=portal-batch-slow-btf"+
                                   "&browseLadder="+encodeURIComponent(cw.bl)+
                                   "&placement=tower"+"&seenasins="+encodeURIComponent(window.pa_asins_tower)+
                                   "&suggestedBN="+encodeURIComponent(suggestedBN)+"&isBrowse="+isBrowse,
                type: "GET",
                cache : false,
                success : function(html) { 
                  jQuery('#click_withinLazyLoad_tower').html(html);    
                },
                error : function(html) { 
                  jQuery('#click_withinLazyLoad_tower').html("<div></div>");    
                }
              }); 
            });
          });
        });
      });
    }



  
